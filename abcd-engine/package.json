{
  "name": "@bhoos/abcd-engine",
  "version": "1.0.0",
  "description": "Abcd Engine",
  "react-native": "src/index.ts",
  "main": "dist/index.js",
  "module": "es6/index.js",
  "author": "Bibek Panthi",
  "types": "./src/index.ts",
  "files": [
    "src",
    "es6",
    "dist"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "test": "vitest",
    "clean": "rimraf dist && rimraf es6",
    "build:cjs": "tsc",
    "build:es6": "mkdir -p es6 && echo '{ \"type\": \"module\" }' > es6/package.json && tsc --module es6 --outDir es6",
    "build": "yarn clean && yarn build:cjs && yarn build:es6",
    "prepublishOnly": "yarn clean && yarn build",
    "pretty": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@bhoos/cards": "^1.0.13",
    "@bhoos/game-kit-engine": "^4.0.0-rc.5",
    "@bhoos/serialization": "^1.0.31"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@bhoos/game-kit-tests": "^4.0.0-rc.3",
    "@types/node": "^18.11.7",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "@vitest/coverage-v8": "^0.33.0",
    "vitest": "^0.33.0"
  }
}
